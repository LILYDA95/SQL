/** Clothing Store:
Crop Tops (5)
Jeans (10)
Jackets (5)
Sweaters (4)
Purses (3)
**/

CREATE TABLE Clothing (Id INTEGER PRIMARY KEY, Item TEXT, Quantity INTEGER, Price INTEGER);

INSERT INTO clothing VALUES(1, "Crop Tops", 5, 10);
INSERT INTO clothing VALUES(2, "Jeans", 10, 30);
INSERT INTO clothing VALUES(3, "Jackets", 5, 40);
INSERT INTO clothing VALUES(4, "Sweaters", 5, 35);
INSERT INTO clothing VALUES(5, "Purses", 3, 20);
SELECT * FROM clothing;

SELECT SUM(price) FROM clothing GROUP BY price;

SELECT SUM(price) FROM Clothing;
SELECT * FROM Clothing;
